MATCH (u:Usuario {nome: "Alice"})-[:OUVIU]->(m:Musica)<-[:OUVIU]-(vizinho:Usuario)
MATCH (vizinho)-[r:OUVIU]->(recomendacao:Musica)
WHERE NOT (u)-[:OUVIU]->(recomendacao)
RETURN recomendacao.titulo AS MusicaSugerida, 
       COUNT(DISTINCT vizinho) AS AmigosQueOuvem, 
       SUM(r.vezes) AS Pontuacao
ORDER BY Pontuacao DESC;
