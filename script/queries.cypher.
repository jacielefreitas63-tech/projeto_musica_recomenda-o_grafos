// Recomendar mÃºsicas de artistas que pessoas com o mesmo gosto que o Carlos ouvem
MATCH (carlos:Usuario {nome: 'Carlos'})-[:OUVIU]->(m:Musica)<-[:OUVIU]-(outro:Usuario)
MATCH (outro)-[:OUVIU]->(rec:Musica)
WHERE NOT (carlos)-[:OUVIU]->(rec)
RETURN DISTINCT rec.titulo AS Sugestao, outro.nome AS Porque_Este_Usuario_Tambem_Ouviu;
